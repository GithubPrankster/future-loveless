[application]

config/name="Future Loveless"
config/description="2 dinguses utilize their skills and magic, a lost power, to defeat the LOVELESS. A strange maniacal lady commands these strange opponents and would rather see our 2 heroes 6 feet under."
run/main_scene="res://src/menu/menu.tscn"
boot_splash/image="res://src/common/squarington.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

